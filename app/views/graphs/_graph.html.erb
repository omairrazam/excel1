<%  @data1  =  graph.graph_datums.pluck(:timestamp_ms,:y1_values) %>
<%  @data2  =  graph.graph_datums.pluck(:timestamp_ms,:y2_values) %>
<%  @divId  = 'container' + graph.id.to_s %>
<% if graph.graph_datums.first.y2_values.present? %>
	<%  @names =   [graph.y1_legend,graph.y2_legend] %>
<% else %>
	<%  @names =   [graph.y1_legend] %>
<% end %>
<% @t = [graph.name] %>


<%= render :partial => "charts", :locals => { :data1 => @data1, :data2 => @data2, :divId => @divId, :names => @names,:tit=> @t} %>